{
    "manifest_version": 3,
    "name": "Translate",
    "description": "Enable focus mode on Chrome's official Extensions and Chrome Web Store documentation.",
    "version": "1.0",
    "icons": {
      "16": "images/16plogo.png",
      "32": "images/32plogo.png",
      "48": "images/48plogo.png",
      "128": "images/128plogo.png"
    },
    "background": {
        "service_worker": "background.js"
      },
      "action": {
        "default_icon": {
          "16": "images/16plogo.png",
          "32": "images/32plogo.png",
          "48": "images/48plogo.png",
          "128": "images/128plogo.png"
        },
        "default_popup": "popup.html"
      },
      "permissions": ["activeTab", "scripting", "contextMenus", "audioCapture"],
      "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
    }
    
}